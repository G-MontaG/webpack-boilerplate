{
  "name": "webpack-boilerplate",
  "version": "1.0.0",
  "description": "webpack-boilerplate",
  "main": "webpack.dev.config.js",
  "scripts": {
    "dev": "webpack --config='webpack.dev.config.js' --progress --display-chunks --display-error-details",
    "dev:hmr": "webpack-dev-server --config webpack.dev.config.js --progress",
    "prod": "webpack --config='webpack.prod.config.js' --progress --display-chunks --display-error-details"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Arthur Osipenko",
  "license": "MIT",
  "devDependencies": {
    "@types/jquery": "^2.0.39",
    "autoprefixer": "^6.7.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "clean-webpack-plugin": "^0.1.15",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "glob": "^7.1.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "imports-loader": "^0.7.0",
    "node-sass": "^4.3.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^4.1.1",
    "source-map-loader": "^0.1.6",
    "tslint": "^4.3.1",
    "tslint-loader": "^3.3.0",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "jquery": "^3.1.1"
  }
}
